    "react-confirm": "^0.1.18",
    "react-datetime": "^2.16.3",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.11.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
